# Copy this file to .env and replace with your actual API keys

# Alchemy API Key - Get one at https://dashboard.alchemy.com/
ALCHEMY_API_KEY=your-alchemy-api-key-here

# API Secret Key for authentication
API_SECRET_KEY=your_secret_here  

# Optional: Ethereum RPC URL (defaults to Alchemy if not specified)
# ETHEREUM_RPC_URL=https://eth-mainnet.alchemyapi.io/v2/your-api-key

# CORS settings (comma-separated list of allowed origins)
CORS_ORIGINS=["http://localhost:3000", "http://127.0.0.1:3000"]

# Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
